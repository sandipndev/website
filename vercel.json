{
  "headers": [
    {
      "source": "/api/youtube/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Authorization, Content-Type"
        },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "POST,OPTIONS" }
      ]
    }
  ]
}
