{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "Youtube Activity Fetcher",
  "description": "Sends your youtube activity to Upstash redis server",
  "permissions": ["activeTab", "alarms"],
  "background": {
    "persistent": false,
    "scripts": ["scripts/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["scripts/content.js"]
    }
  ],
  "browser_action": {}
}
